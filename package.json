{
  "name": "masc-vue",
  "private": false,
  "version": "0.1.8",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/masc-vue.umd.js",
  "module": "./dist/masc-vue.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/masc-vue.es.js",
      "require": "./dist/masc-vue.umd.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "b": "vue-tsc && vite build",
    "build": "vue-tsc && vite build",
    "build:web": "vue-tsc && cross-env WEB=true vite build",
    "build:styles": "postcss src/css/index.css -o dist/index.css",
    "preview": "vite preview",
    "release": "pnpm run build && npm publish --access=public "
  },
  "dependencies": {
    "@iconify/vue": "^4.1.1",
    "@vueuse/core": "^10.4.1",
    "tippy.js": "^6.3.7",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "sass": "^1.68.0",
    "@types/node": "^20.8.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "autoprefixer": "^10.4.16",
    "daisyui": "^3.9.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.0",
    "vue-tsc": "^1.8.15"
  },
  "homepage": "https://masc-vue.netlify.app",
  "bugs": {
    "url": "https://github.com/mohammadsiddiqui/masc/issues",
    "email": "masiddiqui91@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mohammadsiddiqui/masc.git",
    "directory": "src"
  },
  "author": "Mohammad Siddiqui",
  "license": "MIT",
  "keywords": [
    "vue",
    "tailwindcss",
    "daisyui",
    "components",
    "form"
  ]
}
