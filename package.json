{
  "name": "masc-vue",
  "version": "0.0.9",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/masc-vue.umd.js",
  "module": "./dist/masc-vue.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/masc-vue.es.js",
      "require": "./dist/masc-vue.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build  && npm run build:types",
    "build:types": "vue-tsc --project tsconfig.build-types.json --declaration --emitDeclarationOnly --outDir dist/types",
    "build:styles": "postcss src/css/index.css -o dist/index.css",
    "preview": "vite preview",
    "release": "npm run build && npm publish --access=public "
  },
  "dependencies": {
    "vue": "^3.2.47",
    "vue-toast-notification": "3"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@types/node": "^20.2.5",
    "@vitejs/plugin-vue": "^4.1.0",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.0.3",
    "dayjs": "^1.11.8",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vue-final-modal": "^4.4.2",
    "vue-tsc": "^1.4.2"
  }
}
